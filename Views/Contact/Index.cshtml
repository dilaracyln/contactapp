<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>contact</title>
</head>
<body>
    <h1>Contact List</h1>
    <table border="1" cellpadding="2" cellspacing="2">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Surname</th>
            <th>TelNo</th>
        </tr>
        @foreach (var contactItem in ViewBag.contactList)
        {
            <tr>
                <td>@contactItem.Id</td>
                <td>@contactItem.Name</td>
                <td>@contactItem.Surname</td>
                <td>@contactItem.TelNo</td>
            </tr>
        }
    </table>
</body>
</html>     